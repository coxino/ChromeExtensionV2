    "https://mrbit.ro/en/games/*",
    "https://ro.betano.com/casino/games/*",
    "https://conticazino.ro/casino/*",
    "https://online.getsbet.ro/casino/*",
    "https://winboss.ro/en/casino-games/slots/*",
    "https://www.cashpot.ro/games/*"
