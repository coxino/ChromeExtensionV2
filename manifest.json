{
  "manifest_version": 3,
  "name": "CasinoStreamAPI Helper",
  "version": "1.0",
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://developer.chrome.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs","storage",
    "*://*coxino.go.ro:5000/*"
  ]
}
